<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.inheart.inheartapp"
          xmlns:android="http://schemas.android.com/apk/res/android">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name=".activity.user.LoginActivity">
		</activity>
		<activity android:name=".activity.user.RegisterActivity">
		</activity>
		<activity android:name=".Main2Activity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>

				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity android:name=".activity.user.UserInfoCompleteActivity">
		</activity>
		<activity android:name=".InitialActivity">
		</activity>
		<activity android:name=".activity.bill.BillActivity">
		</activity>
	</application>

</manifest>